class Cake {
    taste() {
        var adjective = "delicious";
        print "The " + this.flavor + " cake is " +adjective + "!";
    }
}

class Thing {
    getInnerClass() {
        class InnerClass {
            innerClassPrint() {
                print this;
            }
        }
        return InnerClass;
    }

    getCallback() {
        var localFunction = fun () {
            print this;
        };

        return localFunction;
    }

    class DoTheStaticThing() {
        print "I'm a static method";
    }
}

print Thing().getInnerClass()();
//Thing.WontWork();

var boundStaticMethod = Thing.DoTheStaticThing;
boundStaticMethod();

class Math {
    class square(n) {
        return n * n;
    }

    class cube(n) {
        return n * n * n;
    }

    double(n) {
        return n + n;
    }
}

print Math.square(3);
print Math.cube(3);

var mathInstance = Math();
print mathInstance.double(2);

